package Inheritance2;

public class Cat extends Animal{
	public String food;

	public Cat() {
		this(0,"","");
	}
	public Cat(int age, String name, String food) {
		this.age = age;
		this.name=name;
		this.food = food;
	}
//	public Cat(int age, String name, String food) {
//		super(age, name);
//		this.food = food;
//	}

	@Override
	public void makeNoise() {
		super.makeNoise();
		System.out.println(" Meow Meow Meow");
		System.out.println("Hello I am "+name);
	}
}