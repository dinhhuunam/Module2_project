package Inheritance1;

public class Runner {
	public static void main(String[] args) {
		Student st1 = new Student();
		khoiDong(st1, "A");
		
		Student st2 = new Student("Nguyen An", "Nam");
		System.out.println(st2);
	}
	public static void khoiDong(Student st1, String name) {
		st1.setName(name);
		st1.setStartTime("7h00 AM");
		st1.setStartDo("19h00 PM");
		System.out.println("H·ªçc sinh: "+st1.getName());
		st1.gioDiHoc();
		st1.an();
		st1.gioHocBai();
		st1.ngu();
		System.out.println("-------------------------------");
	}
}