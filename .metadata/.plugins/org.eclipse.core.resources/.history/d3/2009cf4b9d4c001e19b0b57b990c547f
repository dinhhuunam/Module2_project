package BT1;

public class Rectangle {
	private int x;
	private int y;
	private int width;
	private int height;
	
	public Rectangle() {
		this(0, 0,0,0);
	}
	public Rectangle(int x, int y, int width, int height) {
		super();
		this.x = x;
		this.y = y;
		this.width = width;
		this.height = height;
	}
	
}