package Polymorphism;

public class Dog extends Animal{
	private String tiengSua;
	
	public Dog(String tiengSua) {
		super("mammal");
		this.tiengSua=tiengSua;
	}

	@Override
	public void speak() {
//		super.speak();
		System.out.println("I am a "+getType());
		System.out.println("My voice is "+tiengSua);
	}
}