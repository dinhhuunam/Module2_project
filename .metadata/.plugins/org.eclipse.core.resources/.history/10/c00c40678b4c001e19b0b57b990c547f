package Inheritance1;

public class Runner {
	public static void main(String[] args) {
		Student st1 = new Student();
		khoiDong(st1, "A");
	}
	public static void khoiDong(Student st1, String name) {
		st1.setName(name);
		System.out.println("H·ªçc sinh: "+st1.getName());
		st1.gioDiHoc();
		st1.an();
		st1.gioHocBai();
		st1.ngu();
		System.out.println("-------------------------------");
	}
}