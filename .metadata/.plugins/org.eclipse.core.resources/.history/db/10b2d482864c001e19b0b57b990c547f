package Inheritance1;

public class People {
	private String name, gender;
	private int age;
	
	public People(String name, String gender, int age) {
		super();
		this.name = name;
		this.gender = gender;
		this.age = age;
	}
	
	public void an() {
		System.out.println("Cơm là 1 trong những thức ăn của người VIệt Nam.");
	}
	
	public void ngu() {
		System.out.println("Mọi người đều cần phải ngủ để hồi phục sức khỏe.");
	}
}